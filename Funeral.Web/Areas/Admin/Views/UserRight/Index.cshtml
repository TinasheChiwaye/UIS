                    </div>
                    <div class="col-lg-8"></div>
                </div>
                <div class="row form-group">
                    @Html.Label("Company Setup", new { @id = "lblCompany", @class = "col-lg-2" })
                    @if (Model.CompanyStatus)
                    {
                        <div class="col-lg-2">
                            <img src="~/Admin/Images/True.png" class="imagwidth" />
                        </div>
                        <div class="col-lg-8"></div>
                    }
                    else
                    {
                        <div class="col-lg-2">
                            <img src="~/Admin/Images/False.png" style="width:4%" />
                        </div>
                        <div class="col-lg-8"></div>
                    }
