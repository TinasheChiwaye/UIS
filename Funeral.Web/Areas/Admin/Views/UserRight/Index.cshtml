@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Scripts.Render("~/bundles/jquery")

@model Funeral.Model.ProgressStatus
<style>
    .imagwidth {
        width: 10%;
    }
</style>

<div class="row">
    <div class="col-lg-12">
