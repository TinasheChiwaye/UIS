@model Tuple<Tuple<List<string>, List<string>>, List<Funeral.Model.MappedDependents>>
<div class="modal fade" id="dvMappedColumnListPopUp" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times; </button>
                <h4 class="modal-title">Mapped Column</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-sm-6">
                        <h2>Mapped Column</h2>
                        <table class='fixed_header'>
                            <thead>
                                <tr>
                                    <th>Database Column</th>
                                    <th>Excel Column</th>
                                </tr>
                            </thead>
                            <tbody>
                                @{
                                    if (Model.Item1.Item1 != null && Model.Item1.Item2 != null)
                                    {
                                        for (int i = 0; i < Model.Item1.Item1.Count; i++)
                                        {
                                            <tr>
                                                <td>@Model.Item1.Item1[i]</td>
                                                <td>@Model.Item1.Item2[i]</td>
                                            </tr>
                                        }
                                    }
                                    else
                                    {
                                        <tr><td colspan="2">Data not found</td></tr>
                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                    <div class="col-sm-6">
                        <h2>Dependent Type</h2>
                        <table class='fixed_header'>
                            <thead>
                                <tr>
                                    <th>System Member</th>
                                    <th>Excel Member</th>
                                </tr>
                            </thead>
                            <tbody>
                                @{
                                    if (Model.Item2 != null)
                                    {
                                        if (Model.Item2.Count > 0)
                                        {
                                            for (int i = 0; i < Model.Item2.Count; i++)
                                            {
                                                <tr>
                                                    <td>@Model.Item2[i].SystemTypeName</td>
                                                    <td>@Model.Item2[i].ExcelTypeName</td>
                                                </tr>
                                            }
                                        }
                                        else
                                        {
                                            <tr><td colspan="2">Data not found</td></tr>
                                        }
                                    }
                                    else
                                    {
                                        <tr><td colspan="2">Data not found</td></tr>
                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>